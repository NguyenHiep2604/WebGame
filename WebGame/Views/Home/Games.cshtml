@section scripts{
    @*<script>jQuery("#tabs").tabs();</script>*@
    <script>
        var tabsName = document.getElementsByClassName('ui-tabs-tab');
        var tabsContent = document.getElementsByClassName('ui-tabs-panel');
        function showContent(id) {
            for (var i = 0; i < tabsContent.length; i++) {
                tabsContent[i].style.display = 'none';
            }
            var content = document.getElementById(id);
            content.style.display = 'block';
        }
        for (var i = 0; i < tabsName.length; i++) {
            tabsName[i].addEventListener('click', function () {
                var id = this.innerText;
                for (var i = 0; i < tabsName.length; i++) {
                    tabsName[i].classList.remove("ui-tabs-active");
                    tabsName[i].classList.remove("ui-state-active");
                }
                this.className += " ui-tabs-active";
                this.className += " ui-state-active";
                showContent(id);
            })
        }
        showContent('Featured Games');
    </script>
    
    <script>
        var reCaptchaResponse = null;
        isValid = false;
        function verifyCallback(response) {
            isValid = false;
            if (response != null) {
                isValid = true;
                reCaptchaResponse = response;
            }
            validation();
        };

        function changeName() {
            validation();
        }

        function changeEmail() {
            validation();
        }

        function changeGameTitle() {
            validation();
        }

        function changeCompanyName() {
            validation();
        }

        function changeGameURL() {
            validation();
        }

        function changeMore() {
            validation();
        }

        var onloadCallback = function (e) {
            grecaptcha.render('g-recaptcha', {
                'sitekey': '6LfQtmwaAAAAAELVUp_-u9pLQMmQfI4wnt7jQ1eh',
                'callback': verifyCallback,
                'theme': 'dark',
                'expired-callback': expCallback
            });
        };

        var expCallback = function () {
            //grecaptcha.reset();
            isValid = false;
        };

        function validation() {
            var name = $('.FullName').val();
            var email = $('.Email').val();
            var gameTitle = $('.GameTitle').val();
            var companyName = $('.CompanyName').val();
            var gameURL = $('.LinkStore').val();
            var moreAbout = $('.MoreAbout').val();
            if (name != null && name != "" &&
                email != null && email != "" &&
                gameTitle != null && gameTitle != "" &&
                companyName != null && companyName != "" &&
                gameURL != null && gameURL != "" &&
                moreAbout != null && moreAbout != "" &&
                isValid == true) {
                $("#gameSubmit").prop('disabled', false)
            } else {
                $("#gameSubmit").prop('disabled', true)
            }
        }
    </script>
