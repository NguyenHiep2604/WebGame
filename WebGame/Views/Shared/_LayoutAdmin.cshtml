    <link href="~/Content/Data/TrangChu/icon_gangster.png" rel="shortcut icon" type="image/vnd.microsoft.icon">
    <title>Bullet Warriors | Admin</title>
    <link href="~/Content/stylesAdmin.css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
