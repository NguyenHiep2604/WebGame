                            Manage
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayouts" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" href="@Url.Action("Index","Admin")">List Game</a>
                                <a class="nav-link" href="@Url.Action("ListAccount","Admin")">List Account</a>
                                <a class="nav-link" href="@Url.Action("SubmitGame","Admin")">List Game Submit</a>
                                <a class="nav-link" href="@Url.Action("ListJobs","Admin")">List Jobs</a>
                                <a class="nav-link" href="@Url.Action("ListApplyJobs","Admin")">List Apply Jobs</a>
                                <a class="nav-link" href="@Url.Action("ListContact","Admin")">List Contact</a>
                            </nav>
                        </div>
