
@{
    ViewBag.Title = "SubmitGame";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h4 id="ListName"><strong>Danh sách Game Submit</strong></h4>
<div id="gridSubmitGame"></div>

<div id="detailsDialog">
    <div id="detailsForm" novalidate="novalidate" class="detailsForm">
        <input id="ID" name="ID" type="hidden">
        <p><strong>Information:</strong></p>
        <p id="FullName"></p>
        <p id="Email"></p>
        <p id="GameTitle"></p>
        <p id="CompanyGame"></p>
        <p id="Country"></p>
        <p style="margin-top:20px"><strong>Link:</strong></p>
        <p id="VideoFootageLink"></p>
        <p id="LinkAppStore"></p>
        <p style="margin-top:20px"><strong>More about:</strong></p>
        <p id="MoreAbout" style="word-break:break-word"></p>
        <p style="margin-top:20px"><strong>Date submit:</strong></p>
        <p id="DateSubmit"></p>
    </div>
</div>

@section scripts{
    <script src="~/Scripts/submitGameAdmin.js"></script>
}